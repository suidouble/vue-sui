{
  "name": "vue-sui",
  "description": "Vue component to connect your dapp to Sui blockchain",
  "version": "0.0.10",
  "files": ["dist/*"],
  "main": "dist/vue-sui.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "release": "npm run build:component && npm publish",
    "build:component": "vue-cli-service build --target lib --name vue-sui src/vue-sui/index.js"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "suidouble": "^0.0.21"
  },
  "keywords": [
    "sui",
    "sui move",
    "move",
    "smart contract",
    "smart contracts",
    "sui.js",
    "web3",
    "dapps",
    "dapp",
    "vue"
  ],
  "author": "Jeka Kiselyov <jeka911@gmail.com> (https://github.com/jeka-kiselyov)",
  "license": "MIT",
  "devDependencies": {
    "vue": "^3.3.4",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-vue": "^7.0.0",
    "less": "^4.1.2",
    "less-loader": "^6.0.0",
    "suidouble": "^0.0.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  }
}
